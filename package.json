{
  "name": "nodejs-starter",
  "scripts": {
    "build": "babel src -d dist",
    "start": "nodemon --exec babel-node src/index.js",
    "serve": "yarn build && nodemon dist/index.js",
    "test": "mocha --require babel-register",
    "prettier": "prettier --write **/*.js"
  },
  "devDependencies": {
    "@babel/cli": "7.4.4",
    "@babel/core": "7.4.4",
    "@babel/node": "7.2.2",
    "@babel/polyfill": "7.4.4",
    "@babel/preset-env": "7.4.4",
    "@babel/register": "7.4.4",
    "mocha": "6.1.4",
    "nodemon": "1.19.0"
  },
  "dependencies": {
    "cors": "2.8.5",
    "dotenv": "8.0.0",
    "express": "4.16.4"
  }
}
