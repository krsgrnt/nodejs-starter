{
  "name": "nodejs-starter",
  "scripts": {
    "build": "babel src -d dist",
    "start": "nodemon --exec babel-node src/index.js",
    "serve": "yarn build && nodemon dist/index.js",
    "test": "mocha --require babel-register",
    "prettier": "prettier --write **/*.js"
  },
  "devDependencies": {
    "@babel/cli": "^6.26.0",
    "@babel/core": "^7.1.6",
    "@babel/node": "^7.0.0",
    "@babel/polyfill": "^6.26.0",
    "@babel/preset-env": "^7.1.6",
    "@babel/register": "^6.26.0",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.7"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^6.1.0",
    "express": "^4.16.4"
  }
}
