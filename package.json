{
  "name": "nodejs-starter",
  "author": "Kris Grint",
  "scripts": {
    "build": "parcel build ./src/index.js --target node",
    "start": "nodemon --exec babel-node ./src/index.js",
    "test": "mocha --require @babel/register",
    "prettier": "prettier --write ./**/*.js"
  },
  "devDependencies": {
    "@babel/cli": "7.6.0",
    "@babel/core": "7.6.0",
    "@babel/node": "7.6.1",
    "@babel/preset-env": "7.6.0",
    "@babel/register": "7.6.0",
    "mocha": "6.2.0",
    "nodemon": "1.19.2",
    "parcel": "1.12.3",
    "prettier": "1.18.2"
  },
  "dependencies": {
    "cookie-parser": "1.4.4",
    "cors": "2.8.5",
    "dotenv": "8.1.0",
    "express": "4.17.1",
    "express-session": "1.16.2",
    "express-validator": "6.2.0",
    "formidable": "1.2.1",
    "https": "1.0.0"
  }
}
